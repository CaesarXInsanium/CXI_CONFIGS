{
   "author": "Pay With Privacy, Inc.",
   "background": {
      "persistent": true,
      "scripts": [ "js/jquery.min.js", "js/Store.js", "js/api.js", "js/Message.js", "js/Entity.js", "js/background.js" ]
   },
   "browser_action": {
      "default_icon": {
         "16": "assets/icon16.png",
         "32": "assets/icon32.png"
      },
      "default_popup": "popup.html",
      "default_title": "Privacy"
   },
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "js/cookies.min.js", "js/jquery.min.js", "js/crypto.min.js", "js/PaymentCard.js", "js/Checkout.js", "js/Message.js", "js/Entity.js", "js/content.js" ],
      "matches": [ "*://*/*" ],
      "run_at": "document_end"
   } ],
   "content_security_policy": "script-src 'self'; object-src 'self'",
   "description": "Protect yourself from fraud and identity theft with disposable credit card numbers for use online",
   "homepage_url": "https://privacy.com",
   "icons": {
      "128": "assets/icon128.png",
      "16": "assets/icon16.png",
      "48": "assets/icon48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Pay by Privacy.com",
   "permissions": [ "tabs", "*://*/*", "storage" ],
   "short_name": "Pay by Privacy.com",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.5.0",
   "web_accessible_resources": [ "interstitial.html", "assets/icon16.png" ]
}
