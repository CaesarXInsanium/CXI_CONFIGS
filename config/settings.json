{
    "vscode-neovim.neovimExecutablePaths.linux": "/usr/bin/nvim",
    "editor.fontFamily": "Iosevka Term Curly",
    "editor.fontSize": 15,  
    "markdown.preview.fontSize": 15,
    "terminal.integrated.fontSize": 15,
    "editor.fontWeight": 400,
    "editor.fontLigatures": true,
    "terminal.integrated.fontFamily": "",
    "rust-analyzer.inlayHints.smallerHints": false,
    "rust-analyzer.inlayHints.parameterHints": true,
    "rust-analyzer.inlayHints.enable": false,
    "rust-analyzer.cargo.autoreload": true,
    "rust-analyzer.checkOnSave.enable": true,
    "rust-analyzer.inlayHints.maxLength": 25,
    "python.languageServer": "Jedi",
    "python.defaultInterpreterPath": "/home/cxi/CXI_env/bin/python",
    "editor.wordWrap": "on",
    "editor.tokenColorCustomizations": {
       
        "[Tomorrow Night Blue]": {
            "comments": {
            "foreground": "#23deff91",
            "fontStyle": "italic"
        },
        "functions": {
            "foreground": "#19bba8",
            "fontStyle": "italic bold"
        },
        "keywords": {
            "foreground": "#cfcc8a",
            "fontStyle": "italic"
        },
        "numbers": {
            "foreground": "#EE8728",
            "fontStyle": "bold"
        },
        "strings": {
            "foreground": "#23deffa8",
            "fontStyle": "italic"
        },
        "types": {
            "foreground": "#57ADFF"
        },
        "variables": {
            "foreground": "#8EF298",
            "fontStyle": "italic"
        },
        "textMateRules": [
            {
                "scope": "keyword.operator",
                "settings": {
                    "foreground": "#00B7FF"
                }
            },
            {
                "scope": "meta.attribute.rust",
                "settings": {
                    "foreground": "#CD84FF",
                    "fontStyle": ""
                }
            },
            {
                "scope": "keyword.other.rust",
                "settings": {
                    "foreground": "#CD84FF",
                }
            },

            {
                "scope": "entity.name.type",
                "settings":{
                    "foreground": "#57ADFF",
                    "fontStyle": " "
                }
            },
            {
                "scope": "meta.class",
                "settings":{
                    "foreground": "#CD84FF",
                    "fontStyle": "bold "
                }
            },
            {
                "scope": "source.python",
                "settings": {
                    "foreground": "#99f8f8",
                    "fontStyle": ""
                }
            },
            {
                "scope": "meta.function-call",
                "settings": {
                    "foreground": "#e7b6ff",
                    "fontStyle": "italic"
                }
            }
        ]
        
        }
    },
